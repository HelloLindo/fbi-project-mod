{
  "apps": [{
    "name": "app-prod",
    "cwd": "./",
    "script": "index.js",
    "watch": false,
    "max_memory_restart": "1G",
    "exec_mode": "folk",
    "autorestart": true,
    "error_file": "logs/err.log",
    "out_file": "logs/out.log",
    "merge_logs": true,
    "log_date_format": " ",
    "env": {
      "NODE_ENV": "development"
    },
    "env_prod": {
      "NODE_ENV": "production"
    }
  }]
}
